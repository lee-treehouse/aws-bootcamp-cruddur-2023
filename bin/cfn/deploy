#! /bin/bash
set -e

CFN_PATH="$HOME/Developer/tutorials/awsBootcamp/aws-bootcamp-cruddur-2023/aws/cfn/template.yaml"

cfn-lint "$CFN_PATH"

echo "$CFN_PATH"

aws cloudformation deploy \
    --stack-name "mycluster" \
    --template-file "$CFN_PATH" \
    --s3-bucket "cfn-artifacts-leentaylor" \
    --no-execute-changeset \
    --capabilities CAPABILITY_NAMED_IAM
